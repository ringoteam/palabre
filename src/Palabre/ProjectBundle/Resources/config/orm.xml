<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="palabre_project.project_model.class">Palabre\ProjectBundle\Entity\Project</parameter>
    </parameters>
    <services>
        <service id="palabre_project.project_mapper" class="Palabre\ProjectBundle\Repository\ProjectRepository" public="true">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%palabre_project.project_model.class%</argument>
        </service>
    </services>
</container>